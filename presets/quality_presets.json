{
  "quick": {
    "name": "Quick Processing",
    "description": "Fast processing for previews and quick turnaround",
    "processing": {
      "image": {
        "enhancement_level": "light",
        "jpeg_quality": 75,
        "resize_to": [1280, 853]
      },
      "video": {
        "operation": "compress",
        "quality_level": "preview"
      },
      "raw": {
        "preset": "web_optimized",
        "output_format": "JPEG",
        "quality": 75
      }
    },
    "watermark": {
      "enabled": false
    },
    "processing_options": {
      "parallel_processing": true,
      "max_workers": 6,
      "skip_slow_operations": true
    },
    "output": {
      "directory_structure": "quick_preview/{date}",
      "filename_template": "preview_{counter:03d}"
    }
  },

  "standard": {
    "name": "Standard Quality",
    "description": "Balanced quality and processing speed for most use cases",
    "processing": {
      "image": {
        "enhancement_level": "medium",
        "jpeg_quality": 85,
        "maintain_aspect": true
      },
      "video": {
        "operation": "social_media",
        "platform": "website",
        "quality_level": "standard"
      },
      "raw": {
        "preset": "suite_e_event",
        "output_format": "JPEG",
        "quality": 88
      }
    },
    "watermark": {
      "enabled": true,
      "style": "standard"
    },
    "processing_options": {
      "parallel_processing": true,
      "max_workers": 4
    },
    "output": {
      "directory_structure": "standard/{date}",
      "filename_template": "{event_name}_{counter:03d}"
    }
  },

  "professional": {
    "name": "Professional Quality",
    "description": "Maximum quality for important deliverables and client work",
    "processing": {
      "image": {
        "enhancement_level": "heavy",
        "jpeg_quality": 95,
        "maintain_aspect": true
      },
      "video": {
        "operation": "compress",
        "quality_level": "archive"
      },
      "raw": {
        "preset": "high_quality_archive",
        "output_format": "TIFF",
        "quality": 100
      }
    },
    "watermark": {
      "enabled": true,
      "style": "prominent"
    },
    "processing_options": {
      "parallel_processing": false,
      "max_workers": 2,
      "enable_noise_reduction": true,
      "enable_lens_correction": true
    },
    "output": {
      "directory_structure": "professional/{event_name}/{date}",
      "filename_template": "{event_name}_pro_{counter:03d}"
    }
  },

  "archive": {
    "name": "Archive Quality",
    "description": "High quality for long-term storage and future use",
    "processing": {
      "image": {
        "enhancement_level": "light",
        "jpeg_quality": 92,
        "maintain_aspect": true,
        "preserve_original_size": true
      },
      "video": {
        "operation": "compress",
        "quality_level": "archive"
      },
      "raw": {
        "preset": "high_quality_archive",
        "output_format": "TIFF",
        "quality": 98
      }
    },
    "watermark": {
      "enabled": false
    },
    "metadata": {
      "preserve_all_exif": true,
      "add_processing_history": true
    },
    "processing_options": {
      "parallel_processing": true,
      "max_workers": 2,
      "create_backup": true
    },
    "output": {
      "directory_structure": "archive/{year}/{month}/{event_name}",
      "filename_template": "{date}_{event_name}_{counter:03d}"
    }
  },

  "print_ready": {
    "name": "Print Ready",
    "description": "Optimized for high-quality printing at 300 DPI",
    "processing": {
      "image": {
        "enhancement_level": "medium",
        "jpeg_quality": 98,
        "maintain_aspect": true,
        "preserve_original_size": true,
        "color_space": "Adobe RGB",
        "dpi": 300
      },
      "video": {
        "operation": "compress",
        "quality_level": "archive"
      },
      "raw": {
        "preset": "print_ready",
        "output_format": "TIFF",
        "quality": 100,
        "color_space": "Adobe RGB"
      }
    },
    "watermark": {
      "enabled": false
    },
    "metadata": {
      "preserve_all_exif": true,
      "add_color_profile": true
    },
    "processing_options": {
      "parallel_processing": true,
      "max_workers": 2,
      "enable_lens_correction": true
    },
    "output": {
      "directory_structure": "print_ready/{event_name}",
      "filename_template": "{event_name}_print_{counter:03d}"
    }
  },

  "web_optimized": {
    "name": "Web Optimized",
    "description": "Optimized file sizes and formats for web display",
    "processing": {
      "image": {
        "enhancement_level": "medium",
        "jpeg_quality": 78,
        "resize_to": [1920, 1280],
        "output_format": "WEBP"
      },
      "video": {
        "operation": "social_media",
        "platform": "website",
        "quality_level": "standard"
      },
      "raw": {
        "preset": "web_optimized",
        "output_format": "WEBP",
        "quality": 80
      }
    },
    "watermark": {
      "enabled": true,
      "style": "subtle"
    },
    "processing_options": {
      "parallel_processing": true,
      "max_workers": 4,
      "progressive_jpeg": true
    },
    "output": {
      "directory_structure": "web/{date}",
      "filename_template": "web_{event_name}_{counter:03d}"
    }
  }
}
