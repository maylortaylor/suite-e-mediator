{
  "final_friday": {
    "name": "Final Friday",
    "description": "Live music event on the last Friday of each month at Suite E Studios",
    "event_type": "live_music",
    "venue": "Suite E Studios",
    "default_event_name": "Final Friday {month} {year}",
    "processing": {
      "image": {
        "enhancement_level": "medium",
        "venue_lighting": "dim",
        "target_platform": "social_media",
        "maintain_aspect": true,
        "jpeg_quality": 85,
        "format": "JPEG",
        "enhance": true
      },
      "video": {
        "operation": "social_media",
        "platform": "instagram",
        "quality_level": "standard",
        "max_resolution": [
          1080,
          1080
        ],
        "bitrate": "3000k",
        "fps": 30,
        "codec": "h264",
        "format": "MP4"
      },
      "raw": {
        "preset": "suite_e_event",
        "output_format": "JPEG",
        "quality": 90,
        "enhance": true,
        "preserve_original": false
      },
      "audio": {
        "codec": "aac",
        "bitrate": "320k",
        "sample_rate": 44100,
        "channels": 2,
        "volume_normalization": true,
        "enable_loudness_normalization": true,
        "target_lufs": -23.0,
        "max_peak": -1.0,
        "noise_reduction": "light"
      }
    },
    "metadata": {
      "keywords": [
        "final friday",
        "live music",
        "suite e studios",
        "st pete"
      ],
      "event_type": "live_music",
      "copyright": "\u00a9 Suite E Studios",
      "venue": "Suite E Studios",
      "location": "Historic Warehouse Arts District, St. Petersburg, FL"
    },
    "watermark": {
      "enabled": true,
      "style": "social_media",
      "position": "bottom_right",
      "file": "",
      "opacity": 0.3,
      "margin": 100
    },
    "output": {
      "create_subdirectories": true,
      "directory_structure": "{event_name}/{date}/processed",
      "filename_template": "{event_name}_{date}_{artist_names}_{counter:03d}"
    }
  },
  "second_saturday": {
    "name": "Second Saturday Art Walk",
    "description": "Monthly art walk event in the Historic Warehouse Arts District",
    "event_type": "art_walk",
    "venue": "Suite E Studios",
    "default_event_name": "Second Saturday {month} {year}",
    "processing": {
      "image": {
        "enhancement_level": "light",
        "venue_lighting": "natural",
        "target_platform": "website",
        "maintain_aspect": true,
        "jpeg_quality": 88,
        "format": "JPEG",
        "enhance": true
      },
      "video": {
        "operation": "social_media",
        "platform": "facebook",
        "quality_level": "standard",
        "max_resolution": [
          1920,
          1080
        ],
        "bitrate": "3000k",
        "fps": 30,
        "codec": "h264",
        "format": "MP4"
      },
      "raw": {
        "preset": "web_optimized",
        "output_format": "JPEG",
        "quality": 88,
        "enhance": true,
        "preserve_original": false
      },
      "audio": {
        "codec": "aac",
        "bitrate": "320k",
        "sample_rate": 44100,
        "channels": 2,
        "volume_normalization": true,
        "enable_loudness_normalization": true,
        "target_lufs": -23.0,
        "max_peak": -1.0,
        "noise_reduction": "light"
      }
    },
    "metadata": {
      "keywords": [
        "second saturday",
        "art walk",
        "suite e studios",
        "warehouse district"
      ],
      "event_type": "art_walk",
      "copyright": "\u00a9 Suite E Studios",
      "venue": "Suite E Studios",
      "location": "Historic Warehouse Arts District, St. Petersburg, FL"
    },
    "watermark": {
      "enabled": true,
      "style": "standard",
      "position": "bottom_right",
      "file": "",
      "opacity": 0.3,
      "margin": 100
    },
    "output": {
      "create_subdirectories": true,
      "directory_structure": "{event_name}/{date}/processed",
      "filename_template": "{event_name}_{date}_{counter:03d}"
    }
  }
}